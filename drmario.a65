        jsr RB654_NEXT_FRAME                    ; B2FD 20 54 B6
        lda LF5_P1_INPUT_NEW                    ; B300 A5 F5
        ora LF7_P1_INPUT_OLD                    ; B302 05 F7
        and #$10 ; "start"                      ; B304 29 10
    while_eq                                    ; B306 F0 DC
    lda L0727_NUM_PLAYERS                       ; B308 AD 27 07

            lda L47_TEMP                    ; B388 A5 47
            sta L0580_,x                    ; B38A 9D 80 05
            sta L0200_OAM,y                 ; B38D 99 00 02
            inx                             ; B390 E8
            iny                             ; B391 C8
            ; if L43_ & 1 then rotate bottom 2 bits of tile index

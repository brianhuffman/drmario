    ldx L42_OAM_OFFSET                          ; 8906 A6 42
    ldy #$00                                    ; 8908 A0 00
    loop
        lda (L47_TEMP),y                        ; 890A B1 47
        cmp #$80                                ; 890C C9 80
        beq break                               ; 890E F0 2A
        add L45_SPRITE_Y                        ; 8910 18 65 45
        sta L0200_OAM,x                         ; 8913 9D 00 02
        inx                                     ; 8916 E8
